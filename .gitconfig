[user]
	name = Edd Steel
[alias]
	graph = log --branches --color --oneline --graph --decorate
	lg = log --color --oneline
	lgh = !git graph -10 | head -n 10
[color]
	ui = true
[merge]
	tool = vimdiff
[mergetool]
	keepBackup = false
[url "git://github.com/"]
	insteadOf = github://
[url "git@github.com:eddsteel/"]
	pushInsteadOf = git://github.com/eddsteel/
	pushInsteadOf = https://github.com/eddsteel/
	pushInsteadOf = github://eddsteel/
[include]
	path = .gitconfig.extra
[core]
	editor = /usr/bin/vim
	excludesfile = ~/.gitignore-global
